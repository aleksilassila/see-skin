{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "prisma generate && nodemon --files ./src/server.ts",
    "start": "prisma migrate deploy && ts-node --files ./src/server.ts",
    "db:push": "docker exec see-skin-dev-backend npx prisma db push",
    "db:format": "docker exec see-skin-dev-backend npx prisma format",
    "db:studio": "docker exec see-skin-dev-backend npx prisma studio",
    "db:dump": "docker exec -t see-skin-dev-db sh -c 'PGPASSWORD=post pg_dump -U post --insert --data-only post' > ./prisma/seed-dump.sql",
    "backend:sh": "docker exec -it see-skin-dev-backend bash",
    "backend:test": "docker compose exec backend sh -c 'npx prisma migrate reset --force && npx jest'"
  },
  "prisma": {
    "seed": "sh ./prisma/seed.sh"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.23",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.35",
    "bcrypt": "^5.1.0",
    "csv-parse": "^5.3.3",
    "cuid": "^2.1.8",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5"
  }
}
